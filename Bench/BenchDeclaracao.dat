locais:
	real vetor[3];
	logico nope;
comandos:
	vetor[0] := 1.4;
	nope := true;
}

vazio Destruir () {
locais:
	int naoInicializadaNaoReferenciada;
comandos:
	;
}

principal {
locais:
	int naoInicializadaReferenciada;
comandos:
